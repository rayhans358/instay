{
  "version":2,
  "buildCommand": "cd client; npm install; ./node_modules/.bin/vite build",
  "outputDirectory": "client/dist",
  "builds": [
    { "src": "/api/(.*)", 
    "destination": "/api/index.js",
      "use": "@vercel/vite" 
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
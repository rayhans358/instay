{
  "version":2,
  "buildCommand": "cd client; npm install; ./node_modules/.bin/vite build",
  "outputDirectory": "client/dist",
  "rewrites": [
    {
      "source": "/api/(*.)",
      "destination": "/api/index"
    },
    {
      "source": "/api/(*.)",
      "destination": "/api/allRoutes"
    },
    {"source": "/(.*)", "destination": "/"}
  ]
}
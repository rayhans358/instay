{
  "version":2,
  "buildCommand": "cd client; npm install; npx vite build",
  "outputDirectory": "client/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/allRoutes.js"
    },
    {"source": "/(.*)", "destination": "/"}
  ]
}